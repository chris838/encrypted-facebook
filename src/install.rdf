<?xml version="1.0"?>

<!-- Note to translators, these entities are not for translating/changing -->
<!DOCTYPE RDF [
<!ENTITY creator            "Chris Harding">
<!ENTITY homepageURL        "http://cl.cam.ac.uk">
]>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>efb@cl.cam</em:id>
    <em:version>0.1</em:version>
    <em:type>2</em:type>

    <!-- Front End MetaData -->
    <em:creator>Chris Harding</em:creator>
    <em:iconURL>chrome://facebook/skin/fb32.png</em:iconURL>
    <em:optionsURL>chrome://facebook/content/settings.xul</em:optionsURL>
    <em:homepageURL>http://cl.cam.ac.uk</em:homepageURL>

    <!-- Target Application this extension can install into,
         with minimum and maximum supported versions. -->
    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.5</em:minVersion>
        <em:maxVersion>4.0b8</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:localized>
      <Description>
        <em:locale>en-US</em:locale>
        <em:name>Encrypted Facebook</em:name>
        <em:description>Protects your privacy by encrypting data submitted to Facebook</em:description>
        <em:creator>&creator;</em:creator>
        <em:homepageURL>&homepageURL;</em:homepageURL>
      </Description>
    </em:localized>

  </Description>
</RDF>
